client
dev tun
remote vpn.younix.us 1194 udp
nobind
persist-key
persist-tun
comp-lzo
remote-cert-tls server
key-direction 1

redirect-gateway def1

<key>
{{ openvpn_client_key }}
</key>
<cert>
{{ openvpn_client_cert }}
</cert>
<ca>
{{ openvpn_ca_cert }}
</ca>
<dh>
{{ openvpn_dh_param }}
</dh>
<tls-auth>
{{ openvpn_tlsauth_key }}
</tls-auth>
