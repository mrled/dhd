  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.fontFamily": "DejaVu Sans Mono with Numderline, mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/__pycache__": true,
    "**/._*": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": true,
    "**/*.pyc": true,
    "**/CVS": true,
    "**/env": true,
    "**/env.*": true,
    "**/venv": true,
    "**/venv.*": true
  },
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/*.pyc": true,
    "**/env/**": true,
    "**/env.*/**": true,
    "**/venv/**": true,
    "**/venv.*/**": true
  },
  "markdownlint.config": {
    "MD004": false, // don't warn when a list starts with - instead of *
    "MD006": false,
    "MD007": false, // don't warn on 4 space bullet indent
    "MD013": false,
    "MD030": false, // don't warn on 3 spaces after bullet
    "MD033": false // lol yeah absolutely, warn on inline html
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.pyc": true,
    "**/env": true,
    "**/env.*": true,
    "**/venv": true,
    "**/venv.*": true
  },
  "search.useIgnoreFiles": true,
